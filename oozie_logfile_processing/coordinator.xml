<coordinator-app name="logfile-process-coord" frequency="${coord:days(1)}"
                    start="2016-01-01T12:00Z" end="2021-01-01T12:00Z"
                    timezone="America/Los_Angeles"
                    xmlns="uri:oozie:coordinator:0.1">
      <action>
        <workflow>
          <app-path>${namenode}/oozie/logfile-process-cord/wf</app-path>
          <configuration>
            <property>
              <name>jobtracker</name>
              <value>${jobtracker}</value>
            </property>
            <property>
              <name>namenode</name>
              <value>${namenode}</value>
            </property>
            <property>
              <name>inputPath</name>
              <value>${app.inputPath}</value>
            </property>
            <property>
              <name>outputPath</name>
              <value>${app.outputPath}</value>
            </property>
         </configuration>
       </workflow>
      </action>
   </coordinator-app>